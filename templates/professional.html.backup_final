                                                <div class="text-center py-4">
                                                    <div class="spinner-border text-primary" role="status">
                                                        <span class="visually-hidden">Cargando agenda...</span>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        
                                        <!-- Vista Semanal -->
                                        <div id="vistaSemanalContent" class="agenda-view d-none">
                                            <div class="table-responsive">
                                                <table class="table table-bordered agenda-semanal">
                                                    <thead>
                                                        <tr>
                                                            <th style="width: 80px;">Hora</th>
                                                            <th>Lunes</th>
                                                            <th>Martes</th>
                                                            <th>Miércoles</th>
                                                            <th>Jueves</th>
                                                            <th>Viernes</th>
                                                            <th>Sábado</th>
                                                            <th>Domingo</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody id="agendaSemanalBody">
                                                        <!-- El contenido se cargará dinámicamente -->
                                                    </tbody>
                                                </table>
                                            </div>
                                        </div>
                                        
                                        <!-- Vista Mensual -->
                                        <div id="vistaMensualContent" class="agenda-view d-none">
                                            <div class="calendario-mensual" id="calendarioMensual">
                                                <!-- El contenido se cargará dinámicamente -->
                                            </div>
                                        </div>
                                    </div>
                                    </div>
                            
                            <div class="col-lg-4">
                                <!-- Resumen del Día -->
                                <div class="card shadow-sm border-0 mb-4">
                                    <div class="card-header bg-primary text-white">
                                        <h6 class="mb-0 fw-bold">
                                            <i class="fas fa-chart-pie me-2"></i>
                                            Resumen del Día
                                        </h6>
                                    </div>
                                    <div class="card-body p-4">
                                        <div class="row g-3 mb-4">
                                            <div class="col-6">
                                                <div class="text-center p-3 rounded bg-light">
                                                    <div class="h4 mb-1 text-primary fw-bold" id="totalCitas">5</div>
                                                    <small class="text-muted">Total Citas</small>
                                                </div>
                                            </div>
                                            <div class="col-6">
                                                <div class="text-center p-3 rounded bg-light">
                                                    <div class="h4 mb-1 text-success fw-bold" id="confirmadas">2</div>
                                                    <small class="text-muted">Confirmadas</small>
                                                </div>
                                            </div>
                                            <div class="col-6">
                                                <div class="text-center p-3 rounded bg-light">
                                                    <div class="h4 mb-1 text-warning fw-bold" id="pendientes">1</div>
                                                    <small class="text-muted">Pendientes</small>
                                                </div>
                                            </div>
                                            <div class="col-6">
                                                <div class="text-center p-3 rounded bg-light">
                                                    <div class="h4 mb-1 text-info fw-bold" id="disponibles">2</div>
                                                    <small class="text-muted">Disponibles</small>
                                                </div>
                                            </div>
                                        </div>
                                        
                                        <div class="d-grid gap-2">
                                            <button class="btn btn-primary btn-sm" onclick="showScheduleModal()">
                                                <i class="fas fa-plus me-2"></i>
                                                Nueva Cita
                                            </button>
                                            <button class="btn btn-outline-secondary btn-sm" onclick="configurarHorarios()">
                                                <i class="fas fa-clock me-2"></i>
                                                Configurar Horarios
                                            </button>
                                        </div>
                                    </div>
                                </div>
                                
                                <!-- Recordatorios -->
                                <div class="card shadow-sm border-0">
                                    <div class="card-header bg-warning text-dark d-flex justify-content-between align-items-center">
                                        <h6 class="mb-0 fw-bold">
                                            <i class="fas fa-bell me-2"></i>
                                            Recordatorios
