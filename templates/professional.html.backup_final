    </div>

    <!-- Modal para crear/editar recordatorios -->
    <div class="modal fade" id="reminderModal" tabindex="-1" aria-labelledby="reminderModalLabel" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="reminderModalLabel">
                        <i class="fas fa-bell me-2"></i>
                        <span id="reminderModalTitle">Crear Recordatorio</span>
                    </h5>
                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                </div>
                <div class="modal-body">
                    <form id="reminderForm">
                        <input type="hidden" id="reminderId" value="">
                        
                        <div class="form-floating mb-3">
                            <select class="form-select" id="reminderType" required>
                                <option value="">Seleccionar tipo...</option>
                                <option value="confirmacion">Confirmación de Cita</option>
                                <option value="llamada">Llamada al Paciente</option>
                                <option value="preparacion">Preparación Médica</option>
                                <option value="seguimiento">Seguimiento</option>
                                <option value="personal">Recordatorio Personal</option>
                            </select>
                            <label for="reminderType">Tipo de Recordatorio</label>
                        </div>
                        
                        <div class="form-floating mb-3">
                            <select class="form-select" id="reminderPatient">
                                <option value="">Seleccionar paciente (opcional)...</option>
                                <!-- Los pacientes se cargarán dinámicamente -->
                            </select>
                            <label for="reminderPatient">Paciente Relacionado</label>
                        </div>
                        
                        <div class="form-floating mb-3">
                            <input type="text" class="form-control" id="reminderTitle" placeholder="Título del recordatorio" required>
                            <label for="reminderTitle">Título</label>
                        </div>
                        
                        <div class="form-floating mb-3">
                            <textarea class="form-control" id="reminderMessage" style="height: 100px" placeholder="Descripción del recordatorio" required></textarea>
                            <label for="reminderMessage">Descripción</label>
                        </div>
                        
                        <div class="row">
                            <div class="col-md-6">
                                <div class="form-floating mb-3">
                                    <input type="date" class="form-control" id="reminderDate" required>
                                    <label for="reminderDate">Fecha</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating mb-3">
                                    <input type="time" class="form-control" id="reminderTime" required>
                                    <label for="reminderTime">Hora</label>
                                </div>
                            </div>
                        </div>
                        
                        <div class="form-floating mb-3">
                            <select class="form-select" id="reminderPriority">
                                <option value="baja">Baja</option>
                                <option value="media" selected>Media</option>
                                <option value="alta">Alta</option>
                                <option value="urgente">Urgente</option>
                            </select>
                            <label for="reminderPriority">Prioridad</label>
                        </div>
                        
                        <div class="form-check mb-3">
                            <input class="form-check-input" type="checkbox" id="reminderRepeat" value="1">
                            <label class="form-check-label" for="reminderRepeat">
                                Repetir recordatorio
                            </label>
                        </div>
                        
                        <div id="repeatOptions" class="d-none">
                            <div class="form-floating mb-3">
                                <select class="form-select" id="reminderRepeatType">
                                    <option value="diario">Diario</option>
                                    <option value="semanal">Semanal</option>
                                    <option value="mensual">Mensual</option>
                                </select>
                                <label for="reminderRepeatType">Frecuencia</label>
                            </div>
                        </div>
                    </form>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Cancelar</button>
                    <button type="button" class="btn btn-primary" onclick="saveReminder()">
                        <i class="fas fa-save me-2"></i>
                        <span id="saveReminderText">Guardar Recordatorio</span>
                    </button>
                </div>
            </div>
        </div>
    </div>

    <!-- Scripts -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js"></script>
    <script src="https://unpkg.com/leaflet@1.9.4/dist/leaflet.js"></script>
    <script src="{{ url_for('static', filename='js/global-functions.js') }}"></script>
    <script src="{{ url_for('static', filename='js/professional.js') }}?v=1.1"></script>
    <script>
        // Asegurar que las funciones estén disponibles globalmente
        document.addEventListener('DOMContentLoaded', function() {
            // Verificar que las funciones estén disponibles
            if (typeof window.showAddPatientModal === 'undefined') {
                console.error('❌ showAddPatientModal no está disponible');
            } else {
                console.log('✅ showAddPatientModal está disponible');
            }
            
            if (typeof window.viewPatientHistory === 'undefined') {
                console.error('❌ viewPatientHistory no está disponible');
            } else {
                console.log('✅ viewPatientHistory está disponible');
            }
            
            if (typeof window.editPatient === 'undefined') {
                console.error('❌ editPatient no está disponible');
            } else {
                console.log('✅ editPatient está disponible');
            }
            
            if (typeof window.newConsultation === 'undefined') {
                console.error('❌ newConsultation no está disponible');
            } else {
                console.log('✅ newConsultation está disponible');
            }
            
            // Verificar que la variable global esté disponible
            if (typeof window.pacientesList === 'undefined') {
                console.error('❌ pacientesList no está disponible');
            } else {
                console.log('✅ pacientesList está disponible');
            }
            
            // Verificar función de notificación
            if (typeof window.showNotification === 'undefined') {
                console.error('❌ showNotification no está disponible');
            } else {
                console.log('✅ showNotification está disponible');
            }
        });
        
        // Función de fallback para debugging
        window.debugFunction = function(functionName) {
            console.log(`🔍 Verificando función: ${functionName}`);
            if (typeof window[functionName] === 'function') {
                console.log(`✅ ${functionName} está disponible`);
                return true;
            } else {
                console.error(`❌ ${functionName} NO está disponible`);
                return false;
            }
        };
        
        // Prueba completa de todas las funciones
        console.log('🧪 Iniciando pruebas de funciones...');
        
        // Verificar funciones principales
        const funcionesPrincipales = [
            'showAddPatientModal',
            'viewPatientHistory', 
            'editPatient',
            'newConsultation',
            'savePatient',
            'saveAppointment'
        ];
        
        funcionesPrincipales.forEach(funcion => {
            debugFunction(funcion);
        });
        
        // Verificar variables globales
        console.log('🔍 Verificando variables globales...');
        if (typeof window.pacientesList !== 'undefined') {
            console.log('✅ pacientesList está disponible');
        } else {
            console.error('❌ pacientesList NO está disponible');
        }
        
        if (typeof window.showNotification !== 'undefined') {
            console.log('✅ showNotification está disponible');
        } else {
            console.error('❌ showNotification NO está disponible');
        }
        
        console.log('✅ Pruebas de funciones completadas');
        
        // Debug específico para pacientes
        console.log('🔍 Debug de carga de pacientes...');
        
        // Función para probar la carga de pacientes
        window.debugPacientes = function() {
            console.log('🧪 Probando carga de pacientes...');
            
            fetch('/api/professional/patients')
                .then(response => {
                    console.log('📡 Response status:', response.status);
                    console.log('📡 Response headers:', response.headers);
                    return response.json();
                })
                .then(data => {
                    console.log('📥 Datos completos del servidor:', data);
                    
                    if (data.success) {
                        console.log(`✅ ${data.total} pacientes encontrados`);
                        console.log('📋 Lista de pacientes:', data.pacientes);
                        
                        // Verificar si la tabla existe
                        const tabla = document.getElementById('patientsTable');
                        if (tabla) {
                            console.log('✅ Tabla de pacientes encontrada');
                        } else {
                            console.error('❌ Tabla de pacientes NO encontrada');
                        }
                        
                        // Verificar si el tbody existe
                        const tbody = tabla ? tabla.querySelector('tbody') : null;
                        if (tbody) {
                            console.log('✅ Tbody de la tabla encontrado');
                        } else {
                            console.error('❌ Tbody de la tabla NO encontrado');
                        }
                        
                    } else {
                        console.error('❌ Error del servidor:', data.message);
                    }
                })
                .catch(error => {
                    console.error('❌ Error de red:', error);
                });
        };
        
        // Ejecutar debug automáticamente después de 2 segundos
        setTimeout(() => {
            console.log('🔄 Ejecutando debug automático de pacientes...');
            debugPacientes();
        }, 2000);
        
        // Función para probar la base de datos
        window.testDatabase = function() {
            console.log('🧪 Probando conexión con la base de datos...');
            
            // Probar endpoint de health check
            fetch('/health')
                .then(response => response.json())
                .then(data => {
                    console.log('❤️ Health check:', data);
                })
                .catch(error => {
                    console.error('❌ Error en health check:', error);
                });
            
            // Probar endpoint de test de atención
            fetch('/api/test-atencion')
                .then(response => response.json())
                .then(data => {
                    console.log('🧪 Test atenciones:', data);
                })
                .catch(error => {
                    console.error('❌ Error en test atenciones:', error);
                });
        };
        
        // Ejecutar test de base de datos después de 3 segundos
        setTimeout(() => {
            console.log('🔄 Ejecutando test de base de datos...');
            testDatabase();
        }, 3000);
    </script>
</body>
</html>




